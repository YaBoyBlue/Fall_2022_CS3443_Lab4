<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.TrickController">
   <children>
      <VBox alignment="CENTER" maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Trick!" textAlignment="CENTER" wrappingWidth="300.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font name="MV Boli" size="36.0" />
               </font>
            </Text>
            <Text fx:id="question" strokeType="OUTSIDE" strokeWidth="0.0" text="..." textAlignment="CENTER" wrappingWidth="400.0">
               <font>
                  <Font name="MV Boli" size="16.0" />
               </font>
            </Text>
            <Button fx:id="option1" alignment="CENTER" contentDisplay="CENTER" maxHeight="50.0" maxWidth="380.0" minHeight="50.0" minWidth="90.0" mnemonicParsing="false" onAction="#handle" prefHeight="50.0" prefWidth="90.0" text="..." textAlignment="CENTER">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="MV Boli" size="18.0" />
               </font>
            </Button>
            <Button fx:id="option2" alignment="CENTER" contentDisplay="CENTER" maxHeight="50.0" maxWidth="380.0" minHeight="50.0" minWidth="90.0" mnemonicParsing="false" onAction="#handle" prefHeight="50.0" prefWidth="90.0" text="..." textAlignment="CENTER">
               <font>
                  <Font name="MV Boli" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="option3" alignment="CENTER" contentDisplay="CENTER" maxHeight="50.0" maxWidth="380.0" minHeight="50.0" minWidth="90.0" mnemonicParsing="false" onAction="#handle" prefHeight="50.0" prefWidth="90.0" text="..." textAlignment="CENTER">
               <font>
                  <Font name="MV Boli" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Text fx:id="answer" strokeType="OUTSIDE" strokeWidth="0.0" text="..." textAlignment="CENTER" wrappingWidth="380.0">
               <font>
                  <Font name="MV Boli" size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Text>
            <Button fx:id="home" mnemonicParsing="false" onAction="#handle" text="Home">
               <font>
                  <Font name="MV Boli" size="12.0" />
               </font></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
